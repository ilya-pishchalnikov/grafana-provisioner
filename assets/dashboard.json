                {
                    "dataset": "metrics",
                    "datasource": {
                        "type": "grafana-postgresql-datasource",
                        "uid": "${DS_ELMON_METRICS}"
                    },
                    "editorMode": "code",
                    "format": "table",
                    "rawQuery": true,
                    "rawSql": "WITH metric_data AS (\r\n  SELECT\r\n    mv.\"time\" AS time,\r\n    (metric_value ->> 'value')::float AS value\r\n  FROM metric_value mv\r\n  INNER JOIN metric m ON m.metric_id = mv.metric_id\r\n  WHERE\r\n    m.metric_name = 'total_transactions'\r\n    AND mv.server_id = $server\r\n    AND time BETWEEN $__timeFrom() AND $__timeTo()\r\n),\r\ntime_diff AS (\r\n  SELECT\r\n    time,\r\n    value - LAG(value, 1) OVER (ORDER BY time) AS value_diff,\r\n    EXTRACT(EPOCH FROM (time - LAG(time, 1) OVER (ORDER BY time))) AS seconds_diff\r\n  FROM metric_data\r\n)\r\nSELECT\r\n  time,\r\n  value_diff / NULLIF(seconds_diff, 0) AS value -- 'value' is our TPS\r\nFROM time_diff\r\nWHERE\r\n  value_diff >= 0 AND seconds_diff > 0\r\nORDER BY\r\n  time",
                    "refId": "A",
                    "sql": {
                        "columns": [
                            {
                                "parameters": [],
                                "type": "function"
                            }
                        ],
                        "groupBy": [
                            {
                                "property": {
                                    "type": "string"
                                },
                                "type": "groupBy"
                            }
                        ],
                        "limit": 50
                    }
                }
            ],
            "title": "TPS - $server",
            "type": "stat"
        },
